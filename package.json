{
  "name": "problem-test",
  "version": "1.0.0",
  "author": {
    "name": "canals"
  },
  "description": "demo-electron",
  "main": "dist/main.js",
  "scripts": {
    "build:main": "webpack --config webpack.main.config.js",
    "build:renderer": "webpack --config webpack.renderer.config.js",
    "build": "concurrently \"npm run watch:main\" \"npm run watch:renderer\"",
    "start": "npm run build && electron .",
    "watch:main": "webpack --config webpack.main.config.js --watch",
    "watch:renderer": "webpack --config webpack.renderer.config.js --watch",
    "dev": "concurrently \"npm run watch:main\" \"npm run watch:renderer\" \"electron .\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "e-start": "concurrently \"electron .\" \"npm run build:main\" \"npm run build:renderer\"",
    "c-start": "electron .",
    "hot": ".\\node_modules\\.bin\\gulp watch:electron"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@electron/fuses": "^1.8.0",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.2",
    "browserfs": "^1.4.3",
    "concurrently": "^9.1.0",
    "css-loader": "^7.1.2",
    "electron": "^33.2.1",
    "electron-reload": "^2.0.0-alpha.1",
    "gulp": "^5.0.0",
    "html-webpack-plugin": "^5.6.3",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "sass": "^1.82.0",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@monaco-editor/react": "^4.6.0",
    "antd": "^5.22.4",
    "electron-store": "^10.0.0",
    "monaco-editor": "^0.52.2",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
